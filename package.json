{
  "name": "@tillersystems/eslint-config",
  "version": "1.2.1",
  "description": "Shareable ESLint configuration used at Tiller Systems",
  "repository": "tillersystems/tiller-eslint-config ",
  "author": "Thomas Roux <troux@tillersystems.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "tillersystems"
  ],
  "bugs": {
    "url": "https://github.com/tillersystems/tiller-eslint-config/issues"
  },
  "homepage": "https://github.com/tillersystems/tiller-eslint-config#readme",
  "files": [
    "index.js",
    "common.js",
    "react.js",
    "typescript.js"
  ],
  "scripts": {
    "release": "./scripts/release.sh",
    "lint": "eslint .",
    "format": "prettier --config .prettierrc --color --write \"*.js\""
  },
  "bin": {
    "eslint": "node_modules/.bin/eslint",
    "json": "node_modules/.bin/json",
    "prettier": "node_modules/.bin/prettier",
    "pretty-quick": "node_modules/.bin/pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "peerDependencies": {
    "babel-eslint": "^10.x",
    "eslint": "^5.x",
    "eslint-config-airbnb": "^17.x",
    "eslint-config-airbnb-typescript": "^12.x",
    "eslint-config-prettier": "^3.x",
    "eslint-plugin-eslint-comments": "^3.x",
    "eslint-plugin-jest": "^24.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-prettier": "^3.x",
    "eslint-plugin-promise": "^4.x",
    "eslint-plugin-react": "^7.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "babel-eslint": "^10.0.2",
    "conventional-changelog-cli": "^2.0.23",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "3.0.2",
    "json": "^9.0.6",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
